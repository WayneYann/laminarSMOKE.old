FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPointFace;

setFormat       raw;

fields          (p T U H2 O2 H2O N2);

sets
(
    axis
    {
        type    face;
        axis    y;
        start   (1e-10 0      0);
        end     (1e-10 140e-3 0);
    }

    radial_3mm
    {
        type    face;
        axis    x;
        start   (1e-10 3e-3 0);
        end     (20e-3 3e-3 0);
    }

    radial_10mm
    {
        type    face;
        axis    x;
        start   (1e-10 10e-3 0);
        end     (20e-3 10e-3 0);
    }

    radial_20mm
    {
        type    face;
        axis    x;
        start   (1e-10 20e-3 0);
        end     (20e-3 20e-3 0);
    }

    radial_30mm
    {
        type    face;
        axis    x;
        start   (1e-10 30e-3 0);
        end     (20e-3 30e-3 0);
    }

    radial_40mm
    {
        type    face;
        axis    x;
        start   (1e-10 40e-3 0);
        end     (20e-3 40e-3 0);
    }

    radial_50mm
    {
        type    face;
        axis    x;
        start   (1e-10 50e-3 0);
        end     (20e-3 50e-3 0);
    }

    radial_60mm
    {
        type    face;
        axis    x;
        start   (1e-10 60e-3 0);
        end     (20e-3 60e-3 0);
    }

    radial_70mm
    {
        type    face;
        axis    x;
        start   (1e-10 70e-3 0);
        end     (20e-3 70e-3 0);
    }
);



